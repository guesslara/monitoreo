<style type="text/css">
  #menuMapa{
      width:250px;
      position:absolute;     
      border:1px solid black;
      z-index: 9999999; 
      -moz-box-shadow: 0 0 5px #888;
      -webkit-box-shadow: 0 0 5px#888;
      box-shadow: 0 0 5px #888;
      background: #FFF;
  }
  #menuMapa: ul{
      list-style: none;
      list-style-type: none;
      list-style-position: outside;
  }

  #menuMapa: li{
      line-height: 30px;
      font-size: 16px;
      cursor:pointer;
  }
</style>
</head>
<body onload="init();">
<input type="hidden" id="usuarioId" value="{IDUSUARIO}" />
<input type="hidden" id="usuarioCliente" value="{IDCLIENTE}" />
<div id="tabs" class="pestana_principal">
  <ul>
    <li ><a href="#Monitoreo">Monitoreo</a></li>
    <li ><a href="#Admon">Administraci&oacute;n</a></li>
    <li ><a href="#Report">Reportes</a></li>
    <li class="{REPORTS}" ><a href="#DashBoard">DashBoard</a></li>
  </ul>
  <div id="Monitoreo" class="pestana">
    <!--Informacion de las unidades-->
    <div id="contenedorSlider">
	     <div class="divMenu">	    
	       Unidades Disponibles
	     </div>
	     <div id="main_view" class="main_view">
	        <div id="mon_search" style="padding: 5px;position: relative;width: auto;border:1px solid #CCC;">
            <label for="tags">Buscar: </label><input id="tags" />
            <img border="0" src="./public/images/route.png" height="24" width="24" class="imagenRuta" />
            <a href="#" id="btnUltimaPosicion" class="btnSlider ui-state-default ui-corner-all" title="&Uacute;ltima posici&oacute;n">U.Posicion</a>
          </div><!--fin div search-->
          <div style="border:1px solid blue;height:50px;padding:5px;width:95%;margin:2px;">
            <div style="margin:2px;"><input type="text" id="puntoDePartida" placeholder="Punto de partida" /></div>
            <div style="margin:2px;"><input type="text" id="destinoRuta" onkeyup="calcularRuta()" placeholder="Destino" /></div>
          </div>
          <div id="mon_menu_acordeon" class="mon_div_acordeon"></div><!--acordeon de las unidades-->
	     </div><!--Fin del div main_view-->
    </div><!--fin contenedor Slider-->
    <!--Div Pestaña-->
    <div id="btnCejillaOcultar" title="Dar clic para ocultar la barra de unidades">
      <img src="./public/images/ocultar2.png" border="0" style="margin: 7px 0px 0px 2px;" />
    </div>
    <!--fin Div Pestaña-->
    <!--Fin Informacion de las unidades-->
    <!--Div Mapa-->
    <div id="mon_content" class="pestana_monitoreo ui-widget-content"></div>
    <!--fin del Div Mapa-->
    <div id="btnMinimizarUP" title="Minimizar Panel de Ultimas Posiciones"><img src="./public/images/minimizar2.png" border="0" /></div>
    <!--Div Ultimas Posiciones-->
    <div id="divUltimasPosiciones">
      	<div style="border:0px solid #FF0000;height:12px;padding:3px;font-size:9px;" class="fg-toolbar ui-widget-header">
          &Uacute;ltimas Posiciones<!---<a href="#" id="enlaceVentana">Separar</a>-->
        </div>
        <div style="position:relative;border:0px solid #FF0000;height:90%;overflow:auto;">
            <table border="0" cellpadding="0" cellspacing="0" width="99%" id="tablaX" class="tablaUP">
              <tr>
                <th class="ui-state-default" colspan="5">Info</th>
                <th class="ui-state-default">Unidad</th>
                <th class="ui-state-default">Nivel de Bateria</th>
                <th class="ui-state-default">Evento</th>
                <th class="ui-state-default">Tipo Localizaci&oacute;n</th>
                <th class="ui-state-default">Fecha</th>
                <th class="ui-state-default">Velocidad</th>
                <th class="ui-state-default">PDI Cercano</th>
                <th class="ui-state-default">Direcci&oacute;n</th>
                <th class="ui-state-default">Coordenadas</th>
              </tr>
            </table>
        </div>
    </div>
    <!--Fin Div Ultimas Posiciones-->
    <!--Div Geocercas-->
    <div id="mon_Geocercas" class="mon_div_geos">
	    <!--<input type="checkbox" name="mon_chk_g" id="mon_chk_g">GeoPuntos-->
	    <input type="checkbox" name="mon_chk_c" id="mon_chk_c" />GeoCercas
	    <div id="gral_button_events" class="ui-state-default ui-corner-all mon-close-evts" style="margin-top: 29px;width:100px;">
		    <span class="ui-icon ui-icon-alert float_left"></span>
		    <span class="gral_text" style="width: 80px;">Ult. Alertas</span>
	    </div>
    </div>
    <!--Fin Div Geocercas-->
    <!--Div tiempo para actualizar-->
    <div id="mon_TiempoActualizar"  class="mon_div_time_lapse">
      <span>Actualizar en: <span id="mon_time">00:00</span></span>
    </div>
    <div id="divEngraneActualiza" title="Ajustar el Tiempo de Actualizaci&oacute;n"><img src="./public/images/icon-info.png" width="18" height="18" border="0" /></div>
    <div id="cambiarActualizacion">Elija el tiempo para actualizar:
      <select id="mon_sel_time" style="margin:5px 5px 5px 5px;">
	  <option value="10000">10</option>
	  <option value="20000">20</option>
	  <option value="30000">30</option>
	  <option value="60000">60</option>
	  <option value="120000" selected="selected">120</option>
      </select> segs.
      <div style="text-align: right;">
        <a href="#" id="enlaceCerrarAjusteTiempo" title="Cerrar">Cerrar</a>
      </div>
    </div>
    <!--Div tiempo para actualizar-->
  </div><!--Monitoreo-->
  <div id="Admon"  class="pestana"></div><!--Admon-->
  <div id="Report" class="pestana"></div><!--Report-->
  <div id="DashBoard" class="pestana_dashboard"></div><!--DashBoard-->



  
  <!--Texto de Usuario-->
  <div class="mon-user-info">
	  <span><b>Bienvenido(a) : {USER} - <blink>TEST</blink></b></span>
  </div>
  <!--Fin Texto de Usuario-->

  <div id="cargadorGeneral">
    <div class="contenedorCargadorGeneral">
      <div class="imgCargadorGral"><img src='./public/images/mundoM.gif' height="32" width="32" border='0' /></div>
      <div class="textoCargadorGral">Actualizando informaci&oacute;n...</div>
    </div>
  </div>
  <div id="cargador2">
    <div class="imgCargadorGral"><img src='./public/images/mundoM.gif' height="32" width="32" border='0' /></div>
    <div class="textoCargadorGral">Actualizando...</div>
  </div>
  <div id="latlng">&nbsp;</div>
  <div id="error">
    <div class="estilosDivMensajeError">
      <div class="contenedorError">
	       <div id="error_imagen"></div>
	       <div id="error_mensaje" style=""></div>
          <a href="#" id="enlaceGenerarTicket" title="Generar Ticket de Servicio">Generar Ticket de Servicio</a>
          <a id="btnCerrarVentanaError" href="#" style="">Cerrar</a>
          <a id="enlaceCerrarSesion" href="#" style="">Cerrar</a>
      </div>
    </div>
  </div>
  <!--Botones Superiores-->
  <div id="gral_button_soporte" class="ui-state-default ui-corner-all gral_button_soporte">
    <span class="ui-icon  ui-icon-gear float_left"></span>
    <span class="gral_text">Soporte</span>
  </div>
  
  <div id="gral_button_manual" class="ui-state-default ui-corner-all gral_button_manual">
    <span class="ui-icon  ui-icon-folder-collapsed float_left"></span>
    <span class="gral_text">Manual</span>
  </div>
  
  <div id="gral_button_admin" class="ui-state-default ui-corner-all gral_button_admin {S_ADMIN}">
    <span class="ui-icon ui-icon-gear float_left"></span>
    <span class="gral_text">Administración</span>
  </div>
  
  <div id="gral_button_close" class="ui-state-default ui-corner-all mon-close-icon">
    <span class="ui-icon ui-icon-circle-close float_left"></span>
    <span class="gral_text">Cerrar Sesión</span>
  </div>
  <!--Botones Superiores-->
</div><!--tabs-->
